<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>WikiProjectIntegration</display-name>
  <welcome-file-list>
    <welcome-file>wikiConnection.jsp</welcome-file>
  </welcome-file-list>
    <servlet>
    <description></description>
    <display-name>WikiConnector</display-name>
    <servlet-name>WikiConnector</servlet-name>
    <servlet-class>wiki.WikiConnector</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WikiConnector</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
</web-app>